{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "outDir": "build",
    "pretty": true,
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "src", 
    "static/docmaps" 
  ]
}
